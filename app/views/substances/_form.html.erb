
<h2>Create Asset</h2>
<%= form_with model: @substance do |f| %>
  <%= f.label :Add_place_name %><br>
  <%= f.text_field :place %><br>
  <br>
  <%= f.label :Add_Your_Address%><br>
  <%= f.text_area :address%><br>
  <br>
  <%= f.label :category %><br>
<%= f.select(:category, @substance::CLASS_CATEGORY) %>
  <br>  
  <%= f.submit%>
<% end %>